{
	"version": 6,
	"configurePresets": [
		{
			"name": "x64-Debug",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Debug",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_HOME}/scripts/buildsystems/vcpkg.cmake",

				"OPENMS_ENABLE_DISCOVERY": "ON",
				"OPENMS_ENABLE_WEBSERVER": "ON",
				"OPENMS_ENABLE_SAMPLE": "OFF",
				"OPENMS_ENABLE_MYSQL": "OFF",
				"OPENMS_ENABLE_REDIS": "OFF"
			},
			"environment": {
				"VCPKG_HOME": "???"
			}
		},
		{
			"name": "x64-Release",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "RelWithDebInfo",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_HOME}/scripts/buildsystems/vcpkg.cmake",

				"OPENMS_ENABLE_DISCOVERY": "ON",
				"OPENMS_ENABLE_WEBSERVER": "ON",
				"OPENMS_ENABLE_SAMPLE": "OFF",
				"OPENMS_ENABLE_MYSQL": "OFF",
				"OPENMS_ENABLE_REDIS": "OFF"
			},
			"environment": {
				"VCPKG_HOME": "???"
			}
		},
		{
			"name": "x64-Develop",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "RelWithDebInfo",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_HOME}/scripts/buildsystems/vcpkg.cmake",

				"OPENMS_ENABLE_DISCOVERY": "ON",
				"OPENMS_ENABLE_WEBSERVER": "ON",
				"OPENMS_ENABLE_SAMPLE": "ON",
				"OPENMS_ENABLE_MYSQL": "OFF",
				"OPENMS_ENABLE_REDIS": "OFF"
			},
			"environment": {
				"VCPKG_HOME": "F:/Library/Windows/vcpkg-static"
			}
		}
	]
}
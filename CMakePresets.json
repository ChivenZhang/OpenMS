{
	"version": 6,
	"configurePresets": [
		{
			"name": "x64-Debug",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Debug",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",

				"OPENMS_ENABLE_SAMPLE": "ON"
			}
		},
		{
			"name": "x64-Release",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "RelWithDebInfo",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",

				"OPENMS_ENABLE_SAMPLE": "ON"
			}
		},
		{
			"name": "x64-Mingw-Test",
			"generator": "Ninja",
			"binaryDir": "${sourceDir}/Output/${presetName}",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "RelWithDebInfo",
				"CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",
				"OPENMS_ENABLE_SAMPLE": "ON",
				"OPENMS_ENABLE_DISCOVERY": "ON",
				"OPENMS_ENABLE_MYSQL": "ON",
				"OPENMS_ENABLE_REDIS": "ON",
				"MYSQL_CLIENT_CUSTOM": "$env{MYSQL_CLIENT_CUSTOM}",
				"VCPKG_TARGET_TRIPLET": "x64-mingw-static",
				"CMAKE_MAKE_PROGRAM": "ninja.exe",
				"CMAKE_C_COMPILER": "gcc.exe",
				"CMAKE_CXX_COMPILER": "g++.exe"
			}
		}
	]
}